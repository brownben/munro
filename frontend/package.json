{
  "name": "munro",
  "version": "1.0.0",
  "private": false,
  "description": "League Results. Sorted.",
  "author": "brownben <9870007+brownben@users.noreply.github.com>",
  "scripts": {
    "prettier:check": "prettier --check **",
    "prettier:fix": "prettier --write **",
    "typescript:check": "tsc && vue-tsc",
    "eslint:check": "eslint ./src",
    "eslint:fix": "eslint ./src --fix",
    "lint": "npm run prettier:fix && npm run typescript:check && npm run eslint:fix",
    "check": "npm run prettier:check && npm run typescript:check && npm run eslint:check",
    "serve": "node ./localSSR.js",
    "build": "npm run build:client && npm run build:server && npm run build:service-worker",
    "build:client": "vite build --ssrManifest --outDir dist",
    "build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "build:service-worker": "rollup -c service-worker-build.config.js",
    "vercel-build": "npm run build && node ./buildSSR.js"
  },
  "dependencies": {
    "@headlessui/vue": "^1.4.0",
    "@vueuse/head": "^0.6.0",
    "pinia": "^2.0.0-rc.4",
    "register-service-worker": "^1.7.2",
    "vue": "3.2.2",
    "vue-router": "^4.0.10"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@tailwindcss/typography": "^0.4.1",
    "@types/node": "^16.7.13",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "@vitejs/plugin-vue": "^1.3.0",
    "@vue/compiler-sfc": "3.2.2",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/server-renderer": "3.2.2",
    "autoprefixer": "^10.3.1",
    "cssnano": "^5.0.7",
    "cssnano-preset-advanced": "^5.1.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^7.16.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "postcss": "^8.3.6",
    "postcss-preset-env": "^6.7.0",
    "prettier": "2.2.1",
    "rollup": "^2.56.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-workbox-inject": "^2.0.0",
    "tailwindcss": "^2.2.7",
    "typescript": "^4.3.5",
    "vite": "^2.4.4",
    "vue-tsc": "^0.2.2",
    "workbox-cacheable-response": "^6.2.4",
    "workbox-precaching": "^6.2.4",
    "workbox-routing": "^6.2.4",
    "workbox-strategies": "^6.2.4"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "keywords": [
    "sports",
    "sporting",
    "result",
    "results",
    "leagues",
    "league",
    "orienteering",
    "sports-league",
    "orienteering-league",
    "orienteering-results"
  ],
  "license": "MIT",
  "repository": "github:brownben/munro"
}
