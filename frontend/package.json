{
  "name": "munro",
  "version": "1.0.0",
  "private": false,
  "description": "League Results. Sorted.",
  "author": "brownben <9870007+brownben@users.noreply.github.com>",
  "scripts": {
    "prettier:check": "prettier --check **",
    "prettier:fix": "prettier --write **",
    "typescript:check": "tsc && vue-tsc",
    "eslint:check": "eslint ./src",
    "eslint:fix": "eslint ./src --fix",
    "lint": "npm run prettier:fix && npm run typescript:check && npm run eslint:fix",
    "check": "npm run prettier:check && npm run typescript:check && npm run eslint:check",
    "serve": "node ./localSSR.js",
    "build": "npm run build:client && npm run build:server && npm run build:service-worker",
    "build:client": "vite build --ssrManifest --outDir dist",
    "build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "build:service-worker": "rollup -c service-worker-build.config.js",
    "vercel-build": "npm run build && node ./buildSSR.js"
  },
  "dependencies": {
    "@headlessui/vue": "^1.2.0",
    "@vueuse/head": "^0.5.1",
    "pinia": "^2.0.0-alpha.19",
    "register-service-worker": "^1.7.2",
    "vue": "^3.0.11",
    "vue-router": "^4.0.8"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@tailwindcss/typography": "^0.4.1",
    "@types/node": "^15.6.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.11",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/server-renderer": "^3.0.11",
    "autoprefixer": "^10.2.6",
    "cssnano": "^5.0.4",
    "cssnano-preset-advanced": "^5.1.1",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^7.10.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "postcss": "^8.3.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "2.2.1",
    "rollup": "^2.50.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-workbox-inject": "^2.0.0",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.2.4",
    "vite": "^2.3.4",
    "vue-tsc": "^0.1.6",
    "workbox-cacheable-response": "^6.1.5",
    "workbox-precaching": "^6.1.5",
    "workbox-routing": "^6.1.5",
    "workbox-strategies": "^6.1.5"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "keywords": [
    "sports",
    "sporting",
    "result",
    "results",
    "leagues",
    "league",
    "orienteering",
    "sports-league",
    "orienteering-league",
    "orienteering-results"
  ],
  "license": "MIT",
  "repository": "github:brownben/munro"
}
