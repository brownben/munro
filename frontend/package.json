{
  "name": "munro",
  "version": "1.0.0",
  "private": false,
  "description": "League Results. Sorted.",
  "author": "brownben <9870007+brownben@users.noreply.github.com>",
  "scripts": {
    "prettier:check": "prettier --check **",
    "prettier:fix": "prettier --write **",
    "typescript:check": "tsc && vue-tsc",
    "eslint:check": "eslint ./src",
    "eslint:fix": "eslint ./src --fix",
    "lint": "npm run prettier:fix && npm run typescript:check && npm run eslint:fix",
    "check": "npm run prettier:check && npm run typescript:check && npm run eslint:check",
    "generateServiceWorker": "node ./generateSW.js",
    "dev": "vite",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --ssrManifest --outDir dist && node ./generateSW.js",
    "build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "vercel-build": "npm run build && node ./buildSSR.js"
  },
  "dependencies": {
    "@headlessui/vue": "^1.2.0",
    "@vueuse/head": "^0.5.1",
    "pinia": "^2.0.0-alpha.18",
    "register-service-worker": "^1.7.2",
    "vue": "^3.0.11",
    "vue-router": "^4.0.8"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.4.0",
    "@types/node": "^15.3.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.11",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/server-renderer": "^3.0.11",
    "autoprefixer": "^10.2.5",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.3",
    "cssnano-preset-advanced": "^5.0.1",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^7.0.0-beta.4",
    "node-fetch": "^2.6.1",
    "postcss": "^8.2.15",
    "postcss-preset-env": "^6.7.0",
    "prettier": "2.2.1",
    "rollup-plugin-workbox": "^6.1.1",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.2.4",
    "vite": "^2.3.3",
    "vue-tsc": "^0.1.4"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "keywords": [
    "sports",
    "sporting",
    "result",
    "results",
    "leagues",
    "league",
    "orienteering",
    "sports-league",
    "orienteering-league",
    "orienteering-results"
  ],
  "license": "MIT",
  "repository": "github:brownben/munro"
}
